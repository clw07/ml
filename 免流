proxy-groups:
  - name: proxy
    type: select
    proxies:
      - 国外
      - 国外自动
    icon: "https://raw.githubusercontent.com/NB921/picture/main/Golbal.png"

  - name: domestic
    type: select
    proxies:
      - 国内
      - 国内自动
      - proxy
      - DIRECT
    icon: "https://raw.githubusercontent.com/NB921/picture/main/Sdxz.png"

  - name: 国内
    type: select
    <<: *use
    filter: "(?i)CN|徐|武|济"
    icon: "https://raw.githubusercontent.com/NB921/picture/main/Health.png"

  - name: 国内自动
    type: url-test
    <<: *p  # 引用 p 参数
    <<: *use

  - name: 国外
    type: select
    <<: *use
    exclude-filter: "(?i)CN|徐|武|济|州|禁止|官网|以下|剩余|到期|tg"
    icon: "https://raw.githubusercontent.com/NB921/picture/main/Golbal.png"

  - name: 国外自动
    type: url-test
    <<: *p  # 引用 p 参数
    <<: *use
    exclude-filter: "(?i)CN|徐|武|济|州|禁止|官网|以下|剩余|到期|tg"

  - name: 🛑 AdBlock
    type: select
    proxies:
      - REJECT
      - PASS
      - 国外
    icon: "https://raw.githubusercontent.com/NB921/picture/main/Yu.png"

